jQuery(function() {
  $(document).on('ajax:success', '.add a', function(status, data, xhr) {
    var i;
    i = $(this).attr('id');
    return $("#" + i).each(function() {
      var $a, allclass, c, href;
      $a = $(this);
      href = $a.attr('href');
      c = $a.attr('class');
      allclass = c.split('')[0];
      $a.attr('href', $a.data('toggle-href'));
      $a.removeClass(c).addClass($a.data('toggle-class'));
      $a.data('toggle-href', href);
      $a.data('toggle-class', c);
    });
  });
});