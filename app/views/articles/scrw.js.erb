$(document).on('ajax:success', 'a.star', function(status, data, xhr) {
  $("a.star").each(function() {
    var $a, allclass, c, href;
    $a = $(this);
    href = $a.attr('href');
    c = $a.attr('class');
    allclass = c.split('')[0];
    $a.attr('href', $a.data('toggle-href'));
    $a.toggleClass($a.data('toggle-class'));
    $a.data('toggle-href', href);
  });
});

